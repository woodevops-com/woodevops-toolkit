{
    "domain": [
        "pisztrang.com"
    ],
    "root": "/var/www/pisztrang",
    "filename": "pisztrang",
    "block-comments": false,
    "block-xmlrpc": false,
    "cache": [
        "fastcgi_cache_path /usr/share/nginx/fastcgi_cache/pisztrang levels=1:2 keys_zone=pisztrang:100m max_size=10g inactive=10m use_temp_path=off;",
        "fastcgi_cache_key \"$scheme$request_method$host$request_uri$cookie_wcml_client_currency_pisztrang\";",
        "fastcgi_ignore_headers Cache-Control Expires Set-Cookie ;"
    ]
}